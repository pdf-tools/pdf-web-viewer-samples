{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules",
    "assets",
    "build",
    "examples",
    "bin",
    "dist",
    "dist_rel",
    "scripts",
    "lib",
    "webpack.config.js",
    "webpack.config.production.js",
    "babel.config.js"
  ]
}
